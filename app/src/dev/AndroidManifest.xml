<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jaspersoft.android.jaspermobile">

    <permission
        android:name="jaspermobile.util.actions"
        android:label="util_actions" />
    <uses-permission android:name="android.permission.SET_ANIMATION_SCALE"/>

    <application>
        <receiver
            android:name=".UtilReceiver"
            android:exported="true"
            android:permission="jaspermobile.util.actions">
            <intent-filter>
                <action android:name="jaspermobile.util.action.REMOVE_COOKIES" />
                <action android:name="jaspermobile.util.action.REMOVE_ALL_ACCOUNTS" />
                <action android:name="jaspermobile.util.action.INVALIDATE_PASSWORD" />
            </intent-filter>
        </receiver>
    </application>

</manifest>